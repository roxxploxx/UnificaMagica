<?xml version="1.0" encoding="utf-8" ?>
<Defs>


    <ThingDef ParentName="PlantBase">
      <defName>PlantDevilstrandWizard</defName>
      <label>arcane devilstrand</label>
      <description>This fragile mushroom was magically engineered to produce a silk net of the highest grade. Long rumored to have mystical properties, it owes its name to the greed it inspires in people.</description>
      <statBases>
        <MaxHitPoints>85</MaxHitPoints>
        <Beauty>-1</Beauty>
        <Nutrition>0.05</Nutrition>
      </statBases>
      <graphicData>
        <texPath>Things/Plant/Devilstrand</texPath>
        <graphicClass>Graphic_Random</graphicClass>
      </graphicData>
      <selectable>true</selectable>
      <pathCost>10</pathCost>
      <plant>
        <fertilitySensitivity>1.0</fertilitySensitivity>
        <growDays>22.5</growDays>
        <dieIfLeafless>true</dieIfLeafless>
        <!-- reproduces>false</reproduces -->
        <harvestTag>Standard</harvestTag>
        <harvestedThingDef>DevilstrandCloth</harvestedThingDef>
        <harvestAfterGrowth>0</harvestAfterGrowth>
        <harvestYield>8</harvestYield>
        <sowTags>
          <li>Ground</li>
          <li>Hydroponic</li>
        </sowTags>
        <sowMinSkill>10</sowMinSkill>
        <sowResearchPrerequisites>
          <li>Devilstrand</li>
          <li>ArcaneWizardry</li>
        </sowResearchPrerequisites>
        <topWindExposure>0.1</topWindExposure>
        <visualSizeRange>0.3~1.0</visualSizeRange>
      </plant>
    </ThingDef>

</Defs>
