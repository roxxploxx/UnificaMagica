<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <TerrainDef ParentName="FloorBase">
    <defName>ArcaneTile</defName>
    <label>arcane tile</label>
    <renderPrecedence>244</renderPrecedence>
    <description>A wizards preferred footing. Expensive and slow to build, but very impressive. Reduces dirt and clutter magically as well as empowering Arcane Alcoves.</description>
    <texturePath>Terrain/Surfaces/GenericFloorTile</texturePath>
    <color>(0.47, 0.42, 0.57)</color>
    <holdSnow>false</holdSnow>
    <takeFootprints>true</takeFootprints>
    <filthAcceptanceMask><li>None</li></filthAcceptanceMask>
    <statBases>
      <Beauty>5</Beauty>
      <!-- Cleanliness>0.6</Cleanliness -->
      <WorkToBuild>2200</WorkToBuild>
    </statBases>
    <costList>
      <Steel>3</Steel>
      <Silver>15</Silver>
      <PowerRune>1</PowerRune>
      <BindingRune>1</BindingRune>
    </costList>
    <constructEffect>ConstructMetal</constructEffect>
    <designationHotKey>Misc2</designationHotKey>
    <researchPrerequisites>
      <li>ArcaneArtificer</li>
    </researchPrerequisites>
  </TerrainDef>

</Defs>
